{
  "name": "@agent-infra/browser",
  "description": "agent-infra browser",
  "version": "00.2.0-alpha.3",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.node.d.ts",
      "import": "./dist/index.node.mjs",
      "require": "./dist/index.node.js"
    },
    "./web": {
      "types": "./dist/index.web.d.ts",
      "import": "./dist/index.web.mjs",
      "require": "./dist/index.web.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:agent-infra/browser.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@agent-infra/logger": "0.0.2-beta.2",
    "@agent-infra/browser-finder": "workspace:*",
    "@agent-infra/media-utils": "workspace:*",
    "puppeteer-core": "24.23.0",
    "delay": "6.0.0",
    "valtio": "2.1.8",
    "eventemitter3": "5.0.1"
  },
  "devDependencies": {
    "@types/node": "24.7.1",
    "@types/eventemitter3": "2.0.4",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@rslib/core": "0.15.0"
  }
}
