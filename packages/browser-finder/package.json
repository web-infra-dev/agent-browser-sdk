{
  "name": "@agent-infra/browser-finder",
  "description": "find browser in your system",
  "version": "0.1.4",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:agent-infra/browser.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "prepare": "npm run build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "coverage": "vitest run --coverage",
    "test:e2e:local": "vitest --config vitest.e2e.config.ts local-browser.e2e.test.ts"
  },
  "dependencies": {
    "which": "5.0.0",
    "edge-paths": "3.0.5",
    "@agent-infra/logger": "0.0.2-beta.2"
  },
  "devDependencies": {
    "@types/which": "3.0.4",
    "@types/node": "24.1.0",
    "typescript": "5.8.3",
    "vitest": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@rslib/core": "0.11.0"
  }
}
