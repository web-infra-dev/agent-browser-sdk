{
  "name": "@agent-infra/browser-ui",
  "version": "0.2.2",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/agent-browser-sdk.git"
  },
  "files": [
    "dist",
    "examples"
  ],
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "rslib build && rslib build --config rslib.config.bundle.ts",
    "dev": "tsx examples/core/boot.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@agent-infra/browser": "workspace:*",
    "puppeteer-core": "24.23.0",
    "rxjs": "7.8.2",
    "eventemitter3": "5.0.1",
    "lit": "3.3.1"
  },
  "devDependencies": {
    "@rsbuild/core": "1.5.16",
    "@rsbuild/plugin-react": "1.4.1",
    "@rslib/core": "0.15.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@types/eventemitter3": "2.0.4",
    "jsdom": "26.1.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
